{
    "name": "@ayonli/jsext",
    "version": "0.4.7",
    "description": "Additional functions for JavaScript programming in practice.",
    "exports": {
        ".": {
            "require": "./cjs/index.js",
            "import": "./esm/index.js",
            "default": "./index.ts"
        },
        "./augment": {
            "require": "./cjs/augment.js",
            "import": "./esm/augment.js",
            "default": "./augment.ts"
        },
        "./array": {
            "require": "./cjs/array/index.js",
            "import": "./esm/array/index.js",
            "default": "./array/index.ts"
        },
        "./array/augment": {
            "require": "./cjs/array/augment.js",
            "import": "./esm/array/augment.js",
            "default": "./array/augment.ts"
        },
        "./collections": {
            "require": "./cjs/collections/index.js",
            "import": "./esm/collections/index.js",
            "default": "./collections/index.ts"
        },
        "./collections/augment": {
            "require": "./cjs/collections/augment.js",
            "import": "./esm/collections/augment.js",
            "default": "./collections/augment.ts"
        },
        "./collections/BiMap": {
            "require": "./cjs/collections/BiMap.js",
            "import": "./esm/collections/BiMap.js",
            "default": "./collections/BiMap.ts"
        },
        "./collections/CiMap": {
            "require": "./cjs/collections/CiMap.js",
            "import": "./esm/collections/CiMap.js",
            "default": "./collections/CiMap.ts"
        },
        "./error": {
            "require": "./cjs/error/index.js",
            "import": "./esm/error/index.js",
            "default": "./error/index.ts"
        },
        "./error/augment": {
            "require": "./cjs/error/augment.js",
            "import": "./esm/error/augment.js",
            "default": "./error/augment.ts"
        },
        "./error/Exception": {
            "require": "./cjs/error/Exception.js",
            "import": "./esm/error/Exception.js",
            "default": "./error/Exception.ts"
        },
        "./math": {
            "require": "./cjs/math/index.js",
            "import": "./esm/math/index.js",
            "default": "./math/index.ts"
        },
        "./math/augment": {
            "require": "./cjs/math/augment.js",
            "import": "./esm/math/augment.js",
            "default": "./math/augment.ts"
        },
        "./number": {
            "require": "./cjs/number/index.js",
            "import": "./esm/number/index.js",
            "default": "./number/index.ts"
        },
        "./number/augment": {
            "require": "./cjs/number/augment.js",
            "import": "./esm/number/augment.js",
            "default": "./number/augment.ts"
        },
        "./object": {
            "require": "./cjs/object/index.js",
            "import": "./esm/object/index.js",
            "default": "./object/index.ts"
        },
        "./object/augment": {
            "require": "./cjs/object/augment.js",
            "import": "./esm/object/augment.js",
            "default": "./object/augment.ts"
        },
        "./promise": {
            "require": "./cjs/promise/index.js",
            "import": "./esm/promise/index.js",
            "default": "./promise/index.ts"
        },
        "./promise/augment": {
            "require": "./cjs/promise/augment.js",
            "import": "./esm/promise/augment.js",
            "default": "./promise/augment.ts"
        },
        "./string": {
            "require": "./cjs/string/index.js",
            "import": "./esm/string/index.js",
            "default": "./string/index.ts"
        },
        "./string/augment": {
            "require": "./cjs/string/augment.js",
            "import": "./esm/string/augment.js",
            "default": "./string/augment.ts"
        },
        "./uint8array": {
            "require": "./cjs/uint8array/index.js",
            "import": "./esm/uint8array/index.js",
            "default": "./uint8array/index.ts"
        },
        "./uint8array/augment": {
            "require": "./cjs/uint8array/augment.js",
            "import": "./esm/uint8array/augment.js",
            "default": "./uint8array/augment.ts"
        }
    },
    "scripts": {
        "pretest": "node pretest.cjs",
        "posttest": "node posttest.cjs",
        "test": "node --no-warnings=ExperimentalWarning --loader=ts-node/esm ./node_modules/mocha/bin/mocha",
        "test:bun": "bun run ./node_modules/mocha/bin/mocha",
        "build:esm": "rollup -c rollup-esm.config.mjs",
        "build:cjs": "rollup -c rollup-cjs.config.mjs",
        "build:bundle": "rollup -c rollup-bundle.config.mjs",
        "build:worker": "rollup -c rollup-worker.config.mjs",
        "build": "npm run build:esm && npm run build:cjs && npm run build:bundle && npm run build:worker",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ayonli/jsext.git"
    },
    "keywords": [
        "compare",
        "capitalize",
        "hyphenate",
        "chunk",
        "truncate",
        "random",
        "isFloat",
        "count",
        "split",
        "uniq",
        "shuffle",
        "orderBy",
        "groupBy",
        "patch",
        "pick",
        "omit",
        "sum",
        "avg",
        "wrap",
        "mixin",
        "try",
        "defer",
        "sleep",
        "promise",
        "throttle",
        "worker"
    ],
    "author": "A-yon Lee <the@ayon.li>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ayonli/jsext/issues"
    },
    "homepage": "https://github.com/ayonli/jsext#readme",
    "dependencies": {
        "check-iterable": "^1.1.0"
    },
    "devDependencies": {
        "@ayonli/sse": "^0.4.0",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@rollup/plugin-typescript": "^11.1.3",
        "@types/eventsource": "^1.1.11",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.6.0",
        "@types/ws": "^8.5.5",
        "eventsource": "1.1",
        "glob": "^10.3.4",
        "isomorphic-ws": "^5.0.0",
        "magic-string": "^0.30.3",
        "mocha": "^10.2.0",
        "rollup": "^3.29.2",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "tslib": "^2.6.2",
        "typescript": "^5.2.2",
        "ws": "^8.14.1"
    },
    "engines": {
        "node": ">=12"
    },
    "mocha": {
        "spec": [
            "*.test.ts",
            "*/**.test.ts"
        ]
    }
}
