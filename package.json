{
    "name": "@ayonli/jsext",
    "version": "0.2.0",
    "description": "Additional functions for JavaScript builtin types that are frequently used in practice.",
    "main": "index.js",
    "types": "index.ts",
    "scripts": {
        "prepublishOnly": "tsc",
        "test": "mocha",
        "postpublish": "ls *.js|xargs rm; ls *.js.map|xargs rm; ls collections/*.js|xargs rm; ls collections/*.js.map|xargs rm"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ayonli/jsext.git"
    },
    "keywords": [
        "compare",
        "capitalize",
        "hyphenate",
        "chunk",
        "truncate",
        "random",
        "isFloat",
        "count",
        "split",
        "uniq",
        "shuffle",
        "orderBy",
        "groupBy",
        "patch",
        "pick",
        "omit",
        "sum",
        "avg",
        "wrap",
        "try",
        "sleep",
        "promise"
    ],
    "author": "A-yon Lee <the@ayon.li>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ayonli/jsext/issues"
    },
    "homepage": "https://github.com/ayonli/jsext#readme",
    "dependencies": {
        "check-iterable": "^1.1.0",
        "tslib": "^2.6.2"
    },
    "devDependencies": {
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.6.0",
        "mocha": "^10.2.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=14"
    },
    "mocha": {
        "require": [
            "ts-node/register"
        ],
        "spec": [
            "*.test.ts",
            "**/*.test.ts"
        ]
    }
}
