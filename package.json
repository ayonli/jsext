{
    "name": "@ayonli/jsext",
    "version": "0.4.0",
    "description": "Additional functions for JavaScript programming in practice.",
    "main": "index.js",
    "scripts": {
        "test": "mocha",
        "postpublish": "ls *.js|grep -v config.js|xargs rm; ls *.js.map|xargs rm; ls */*.js|grep -v bundle|xargs rm; ls */*.js.map|grep -v bundle|xargs rm",
        "build": "tsc && rollup -c rollup.config.mjs && webpack",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/ayonli/jsext.git"
    },
    "keywords": [
        "compare",
        "capitalize",
        "hyphenate",
        "chunk",
        "truncate",
        "random",
        "isFloat",
        "count",
        "split",
        "uniq",
        "shuffle",
        "orderBy",
        "groupBy",
        "patch",
        "pick",
        "omit",
        "sum",
        "avg",
        "wrap",
        "mixin",
        "try",
        "defer",
        "sleep",
        "promise",
        "throttle",
        "worker"
    ],
    "author": "A-yon Lee <the@ayon.li>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ayonli/jsext/issues"
    },
    "homepage": "https://github.com/ayonli/jsext#readme",
    "dependencies": {
        "check-iterable": "^1.1.0",
        "tslib": "^2.6.2"
    },
    "devDependencies": {
        "@ayonli/sse": "^0.4.0",
        "@rollup/plugin-commonjs": "^25.0.4",
        "@rollup/plugin-node-resolve": "^15.2.1",
        "@types/eventsource": "^1.1.11",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.6.0",
        "@types/websocket": "^1.0.6",
        "builtin-modules": "^3.3.0",
        "eventsource": "1.1",
        "mocha": "^10.2.0",
        "rollup": "^3.29.2",
        "ts-loader": "^9.4.4",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4",
        "websocket": "^1.0.34"
    },
    "engines": {
        "node": ">=14"
    },
    "mocha": {
        "require": [
            "ts-node/register"
        ],
        "spec": [
            "**/*.test.ts"
        ]
    }
}
